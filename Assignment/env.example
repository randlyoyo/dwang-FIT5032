# EmailJS配置 - 安全配置示例
# 复制此文件为 .env.local 并填入你的实际配置

# EmailJS配置 (Vite环境变量以VITE_开头)
VITE_EMAILJS_SERVICE_ID=your_service_id_here
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# 安全配置
VITE_EMAILJS_ENABLE_DOMAIN_WHITELIST=true
VITE_EMAILJS_MAX_REQUESTS_PER_MINUTE=5
VITE_EMAILJS_ENABLE_CONTENT_FILTER=true

# Google Cloud Platform配置
VITE_GCP_PROJECT_ID=your-project-id
VITE_GCP_BUCKET_NAME=your-bucket-name
VITE_GCP_API_ENDPOINT=http://localhost:3000/api

# Google Maps API配置
# 获取API Key: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Mapbox配置 (推荐使用，比Google Maps更简单)
# 获取Token: https://account.mapbox.com/access-tokens/
VITE_MAPBOX_TOKEN=your_mapbox_token_here

# Google Gemini API配置 (GenAI集成)
# 免费获取API Key: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=your_gemini_api_key_here
